<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>returncycle</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>MemberFrontController</servlet-name>
    <servlet-class>com.koreait.app.member.MemberFrontController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>MemberFrontController</servlet-name>
    <url-pattern>*.me</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ApplyFrontController</servlet-name>
    <servlet-class>com.koreait.app.apply.ApplyFrontController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ApplyFrontController</servlet-name>
    <url-pattern>*.ap</url-pattern>
  </servlet-mapping>
  	<servlet>
		<servlet-name>DeliverFrontController</servlet-name>
		<servlet-class>com.koreait.app.Deliver.DeliverFrontController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>DeliverFrontController</servlet-name>
		<url-pattern>*.do</url-pattern>
	</servlet-mapping>
	<servlet>
	    <servlet-name>PaybackFrontController</servlet-name>
	    <servlet-class>com.koreait.app.payback.PaybackFrontController</servlet-class>
  	</servlet>
  	<servlet-mapping>
	    <servlet-name>PaybackFrontController</servlet-name>
	    <url-pattern>*.py</url-pattern>
  	</servlet-mapping>
  	<servlet>
		<servlet-name>CodeFrontController</servlet-name>
		<servlet-class>com.koreait.app.code.CodeFrontController</servlet-class>
 	</servlet>
  	<servlet-mapping>
	    <servlet-name>CodeFrontController</servlet-name>
	    <url-pattern>*.cd</url-pattern>
  	</servlet-mapping>
  	<servlet>
	    <servlet-name>ReviewFrontController</servlet-name>
	    <servlet-class>com.koreait.app.review.ReviewFrontController</servlet-class>
  </servlet>
  <servlet-mapping>
	    <servlet-name>ReviewFrontController</servlet-name>
	    <url-pattern>*.rv</url-pattern>
  </servlet-mapping> 
</web-app>